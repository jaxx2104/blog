{
  "name": "blog",
  "description": "Simple starter for Gatsby",
  "version": "1.9.119",
  "author": "Futoshi Iwashita <jaxx2104@gmail.com>",
  "dependencies": {
    "gatsby": "2.13.25",
    "gatsby-image": "2.2.6",
    "gatsby-link": "2.2.2",
    "gatsby-plugin-catch-links": "2.1.2",
    "gatsby-plugin-feed": "2.3.4",
    "gatsby-plugin-google-analytics": "2.1.4",
    "gatsby-plugin-manifest": "2.2.3",
    "gatsby-plugin-netlify": "2.1.3",
    "gatsby-plugin-nprogress": "2.1.2",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-preact": "3.1.4",
    "gatsby-plugin-react-helmet": "3.1.2",
    "gatsby-plugin-sharp": "2.2.7",
    "gatsby-plugin-sitemap": "2.2.3",
    "gatsby-plugin-styled-components": "3.1.2",
    "gatsby-plugin-twitter": "2.1.2",
    "gatsby-remark-copy-linked-files": "2.1.3",
    "gatsby-remark-images": "3.1.6",
    "gatsby-remark-prismjs": "3.3.3",
    "gatsby-remark-responsive-iframe": "2.2.4",
    "gatsby-remark-smartypants": "2.1.2",
    "gatsby-source-filesystem": "2.1.5",
    "gatsby-transformer-remark": "2.6.6",
    "gatsby-transformer-sharp": "2.2.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-brands-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "animate.css": "3.7.2",
    "babel-eslint": "10.0.2",
    "babel-plugin-styled-components": "1.10.6",
    "emergence.js": "1.1.2",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "font-awesome": "4.7.0",
    "gh-pages": "2.0.1",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "localforage": "1.7.3",
    "lodash": "4.17.14",
    "modern-normalize": "0.5.0",
    "prettier": "1.18.2",
    "prismjs": "1.16.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-lazyload": "2.6.2",
    "react-share": "3.0.0",
    "styled-components": "4.3.2",
    "textlint": "11.3.0",
    "textlint-rule-preset-ja-spacing": "2.0.1",
    "textlint-rule-preset-japanese": "4.0.3"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "env NODE_PATH=./src/components gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\" --ignore-path .gitignore",
    "lint:text": "textlint content/**/index.md",
    "lint:textfix": "textlint --fix content/**/index.md",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "storybook": "start-storybook -p 6006",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "npm run lint:textfix",
      "git add"
    ]
  }
}
