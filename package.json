{
  "name": "blog",
  "description": "Simple starter for Gatsby",
  "version": "1.9.119",
  "author": "Futoshi Iwashita <jaxx2104@gmail.com>",
  "dependencies": {
    "gatsby": "2.23.7",
    "gatsby-image": "2.4.7",
    "gatsby-link": "2.4.6",
    "gatsby-plugin-catch-links": "2.3.5",
    "gatsby-plugin-feed": "2.5.5",
    "gatsby-plugin-google-analytics": "2.3.4",
    "gatsby-plugin-graphql-codegen": "2.7.1",
    "gatsby-plugin-manifest": "2.4.12",
    "gatsby-plugin-netlify": "2.3.5",
    "gatsby-plugin-nprogress": "2.3.4",
    "gatsby-plugin-offline": "3.2.11",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-sharp": "2.6.12",
    "gatsby-plugin-sitemap": "2.4.5",
    "gatsby-plugin-styled-components": "3.3.4",
    "gatsby-plugin-twitter": "2.3.4",
    "gatsby-remark-copy-linked-files": "2.3.5",
    "gatsby-remark-images": "3.3.11",
    "gatsby-remark-prismjs": "3.5.4",
    "gatsby-remark-responsive-iframe": "2.4.5",
    "gatsby-remark-smartypants": "2.3.4",
    "gatsby-source-filesystem": "2.3.12",
    "gatsby-transformer-remark": "2.8.17",
    "gatsby-transformer-sharp": "2.5.5"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.29",
    "@fortawesome/free-brands-svg-icons": "5.13.1",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@types/react-helmet": "6.0.0",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "7.3.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "font-awesome": "4.7.0",
    "gh-pages": "3.1.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "localforage": "1.7.4",
    "modern-normalize": "0.6.0",
    "prettier": "2.0.5",
    "prismjs": "1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-lazyload": "2.6.8",
    "react-share": "4.2.0",
    "styled-components": "5.1.1",
    "textlint": "11.6.3",
    "textlint-rule-preset-ja-spacing": "2.0.1",
    "textlint-rule-preset-japanese": "5.0.0",
    "typescript": "3.9.5"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "env NODE_PATH=./src/components gatsby develop",
    "format": "prettier --write \"**/*.+(ts|tsx|json|md)\" --ignore-path .gitignore",
    "lint:text": "textlint content/**/index.md",
    "lint:textfix": "textlint --fix content/**/index.md",
    "lint": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore src",
    "test": "npx tsc -p ./tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "npm run lint:textfix",
      "git add"
    ]
  }
}
