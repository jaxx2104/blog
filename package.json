{
  "name": "blog",
  "description": "Simple starter for Gatsby",
  "version": "1.9.119",
  "author": "Futoshi Iwashita <jaxx2104@gmail.com>",
  "dependencies": {
    "gatsby": "2.24.65",
    "gatsby-image": "2.4.19",
    "gatsby-link": "2.4.14",
    "gatsby-plugin-catch-links": "2.3.12",
    "gatsby-plugin-feed": "2.5.12",
    "gatsby-plugin-google-analytics": "2.3.14",
    "gatsby-plugin-manifest": "2.4.31",
    "gatsby-plugin-netlify": "2.3.15",
    "gatsby-plugin-nprogress": "2.3.11",
    "gatsby-plugin-offline": "3.2.28",
    "gatsby-plugin-react-helmet": "3.3.11",
    "gatsby-plugin-sharp": "2.6.36",
    "gatsby-plugin-sitemap": "2.4.13",
    "gatsby-plugin-styled-components": "3.3.11",
    "gatsby-plugin-twitter": "2.3.11",
    "gatsby-plugin-typegen": "^2.2.1",
    "gatsby-remark-copy-linked-files": "2.3.16",
    "gatsby-remark-images": "3.3.30",
    "gatsby-remark-prismjs": "3.5.13",
    "gatsby-remark-responsive-iframe": "2.4.14",
    "gatsby-remark-smartypants": "2.3.11",
    "gatsby-source-filesystem": "2.3.30",
    "gatsby-transformer-remark": "2.8.35",
    "gatsby-transformer-sharp": "2.5.15"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.31",
    "@fortawesome/free-brands-svg-icons": "5.15.0",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.3",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "font-awesome": "4.7.0",
    "gh-pages": "3.1.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "localforage": "1.9.0",
    "modern-normalize": "1.0.0",
    "prettier": "2.1.2",
    "prismjs": "1.21.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-lazyload": "3.0.0",
    "react-share": "4.3.0",
    "styled-components": "5.2.0",
    "textlint": "11.7.6",
    "textlint-rule-preset-ja-spacing": "2.0.2",
    "textlint-rule-preset-japanese": "5.0.0",
    "typescript": "4.0.3"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "env NODE_PATH=./src/components gatsby develop",
    "format": "prettier --write \"**/*.+(ts|tsx|json|md)\" --ignore-path .gitignore",
    "lint:text": "textlint content/**/index.md",
    "lint:textfix": "textlint --fix content/**/index.md",
    "lint": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore src",
    "test": "npx tsc -p ./tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "npm run lint:textfix",
      "git add"
    ]
  }
}
